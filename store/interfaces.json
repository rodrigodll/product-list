{
  "product-list-edit": {
    "component": "ProductListWrapper",
    "allowed": ["product-list-content-desktop", "product-list-content-mobile"]
  },
  "product-list-content-desktop-edit": {
    "component": "ProductList",
    "composition": "children",
    "allowed": "*"
  },
  "product-list-content-mobile-edit": {
    "component": "ProductList",
    "composition": "children",
    "allowed": "*"
  },
  "product-list-image-edit": {
    "component": "Image",
    "preview": {
      "type": "box",
      "height": 96,
      "width": 96,
      "options": {
        "padding": 0,
        "minWidth": 96
      }
    }
  },
  "message-edit": {
    "component": "AvailabilityMessage"
  },
  "product-name-edit": {
    "component": "ProductName",
    "preview": {
      "type": "box",
      "height": 30,
      "width": 140,
      "options": {
        "padding": 0
      }
    }
  },
  "product-reference-edit": {
    "component": "ProductReference"
  },
  "product-brand-edit": {
    "component": "ProductBrand",
    "preview": {
      "type": "box",
      "height": 15,
      "width": 110,
      "options": {
        "padding": 0
      }
    }
  },
  "product-variations-edit": {
    "component": "ProductVariations"
  },
  "product-quantity-label-edit": {
    "component": "ProductQuantityLabel"
  },
  "quantity-selector-edit": {
    "component": "QuantitySelector",
    "preview": {
      "type": "box",
      "height": 35,
      "width": 80,
      "options": {
        "padding": 0
      }
    }
  },
  "unit-price-edit": {
    "component": "UnitPrice"
  },
  "price-edit": {
    "component": "Price",
    "preview": {
      "type": "box",
      "height": 35,
      "width": 90,
      "options": {
        "padding": 0
      }
    }
  },
  "remove-button-edit": {
    "component": "RemoveButton",
    "preview": {
      "type": "box",
      "height": 25,
      "width": 25,
      "options": {
        "padding": 0
      }
    }
